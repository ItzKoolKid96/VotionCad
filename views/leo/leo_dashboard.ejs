            document.getElementById("108").disabled = true;
            document.getElementById("107").className = "h-12 px-6 m-2 text-lg text-indigo-100 transition-colors duration-150 bg-indigo-700 rounded-lg focus:shadow-outline hover:bg-indigo-800";
            document.getElementById("107").disabled = false;
